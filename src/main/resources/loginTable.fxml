<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="265.0" prefWidth="320.0" style="-fx-background-color: #ffff;"
            stylesheets="@color.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hellofx.LoginController">
   <JFXTextField fx:id="username" focusColor="#330fdb" labelFloat="true" layoutX="40.0" layoutY="31.0"
                 onAction="#actionLogin" prefHeight="27.0" prefWidth="184.0" promptText="UserName"/>
   <JFXPasswordField fx:id="password" focusColor="#330fdb" labelFloat="true" layoutX="45.0" layoutY="91.0"
                     onAction="#actionLogin" promptText="Password"/>
   <Hyperlink layoutX="45.0" layoutY="149.0" onAction="#OpenReg" text="Register"/>
   <JFXButton fx:id="ButtonLogin" buttonType="RAISED" layoutX="45.0" layoutY="194.0" onAction="#actionLogin"
              prefHeight="18.0" prefWidth="74.0" styleClass="ButtonLogin" stylesheets="@color.css" text="Login">
      <font>
         <Font size="14.0"/>
      </font>
   </JFXButton>
   <JFXButton fx:id="CancelSingIn" buttonType="RAISED" layoutX="196.0" layoutY="194.0" onAction="#CancelSingIn"
              styleClass="ButtonCancel" stylesheets="@color.css" text="Cancel"/>
   <Hyperlink layoutX="196.0" layoutY="149.0" onAction="#CreateTable" text="Settings"/>
</AnchorPane>
