<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="180.0" prefWidth="280.0" styleClass="my-pane" stylesheets="@color.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.LoginController">
   <TextField fx:id="username" layoutX="46.0" layoutY="29.0" onAction="#actionLogin" prefHeight="27.0" prefWidth="184.0" promptText="UserName" styleClass="textfield" stylesheets="@color.css" />
   <PasswordField fx:id="password" layoutX="47.0" layoutY="65.0" onAction="#actionLogin" promptText="Password" styleClass="textfield" stylesheets="@color.css" />
   <Hyperlink layoutX="68.0" layoutY="95.0" onAction="#OpenReg" text="Register" />
   <Button fx:id="ButtonLogin" layoutX="46.0" layoutY="126.0" onAction="#actionLogin" prefHeight="18.0" prefWidth="75.0" styleClass="ButtonLogin" stylesheets="@color.css" text="Login">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button fx:id="CancelSingIn" layoutX="166.0" layoutY="126.0" onAction="#CancelSingIn" styleClass="ButtonCancel" stylesheets="@color.css" text="Cancel" />
   <Hyperlink layoutX="171.0" layoutY="95.0" onAction="#CreateTable" text="Settings" />
</AnchorPane>
