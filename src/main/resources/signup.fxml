<AnchorPane fx:id="signIn" prefHeight="450.0" prefWidth="365.0" styleClass="my-paneTestUp" stylesheets="@color.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.SignController">

   <TextField fx:id="Username" layoutX="69.0" layoutY="103.0" prefHeight="27.0" prefWidth="221.0" promptText="Username" />
   <PasswordField fx:id="Password" layoutX="69.0" layoutY="164.0" promptText="Password" />
   <TextField fx:id="email" layoutX="69.0" layoutY="219.0" prefHeight="15.0" prefWidth="151.0" promptText="email" />
   <TextField fx:id="code" layoutX="69.0" layoutY="284.0" promptText="Verification code" />
   <Button layoutX="65.0" layoutY="375.0" onAction="#Back" prefHeight="37.0" prefWidth="87.0" styleClass="my-button" stylesheets="@color.css" text="Back" />
   <Button layoutX="220.0" layoutY="375.0" onAction="#SubmitDB" prefHeight="37.0" prefWidth="87.0" styleClass="submit" stylesheets="@color.css" text="Submit" />
   <Text fx:id="invalidData" fill="#ee0808" layoutX="33.0" layoutY="347.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="300.0" />
   <Text fill="#90c404" layoutX="69.0" layoutY="62.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration">
      <font>
         <Font size="39.0" />
      </font>
   </Text>
   <Label fx:id="notification" layoutX="72.0" layoutY="137.0" prefHeight="17.0" prefWidth="222.0" textFill="RED" />
